<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>IndexServlet</servlet-name>
        <servlet-class>org.su18.memshell.test.websphere.IndexServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>AddWebsphereFilter</servlet-name>
        <servlet-class>org.su18.memshell.test.websphere.AddWebsphereFilter</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>IndexServlet</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>


    <servlet-mapping>
        <servlet-name>AddWebsphereFilter</servlet-name>
        <url-pattern>/addWebsphereFilter</url-pattern>
    </servlet-mapping>
</web-app>